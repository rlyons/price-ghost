# API Keys for Price Ghost
# Copy this file to .env and fill in your actual API keys

# Keepa API for price history data
KEEPA_API_KEY=your_keepa_api_key_here

# UPCitemdb API for fallback product lookup
UPCITEMDB_KEY=your_upcitemdb_api_key_here

# Barcode Lookup API for fallback product lookup
BARCODE_LOOKUP_KEY=your_barcode_lookup_api_key_here

# Supabase configuration (optional, for server-side watchlist)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here